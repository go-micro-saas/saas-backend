.PHONY: rm-makefile
# rm makefile
rm-makefile:
	@echo "==> find files: "$(shell find ./ -name "*.mk")
	@echo $(shell find ./ -name "*.mk" -delete)

.PHONY: rm-gofile
# rm makefile
rm-go-file:
	@echo "==> find files: "$(shell find ./ -name "*.go")
	@echo $(shell find ./ -name "*.go" -delete)

.PHONY: rm-swagger-file
# rm makefile
rm-swagger-file:
	@echo "==> find files: "$(shell find ./ -name "*.swagger.json")
	@echo $(shell find ./ -name "*.swagger.json" -delete)
